{{#each categories}}
  <div class="box box-link-hover-shadow">
    <h2 class="title">{{type}}</h2>
    <a href="/category/{{_id}}" class="button">
      <span class="icon is-small">
        <i class="fas fa-folder-open"></i>
        </span>&emsp;View Points of Interest
    </a>
    <a href="/dashboard/delete-category/{{_id}}" class="button">
      <i class="fas fa-trash"></i>&emsp;Delete Category
    </a>
  </div>
{{/each}}
